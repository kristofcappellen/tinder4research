  
{
    "welcomeFile": "uimodule/index.html",
    "authenticationMethod": "none",
    "logout": {
      "logoutEndpoint": "/do/logout"
    },
    "routes": [
      {
        "source": "^/uimodule/research/(.*)$",
        "target": "/research/$1",
        "authenticationType": "none",
        "destination": "srv_api",
        "csrfProtection": true
      },      
      {
        "source": "^/appdebug/research/(.*)$",
        "target": "/research/$1",
        "authenticationType": "none",
        "destination": "srv_api",
        "csrfProtection": true
      },
      {
        "source": "^/uimodule/(.*)$",
        "target": "$1",
        "localDir": "dist"
      },
      {
        "source": "^/appdebug/(.*)$",
        "target": "$1",
        "localDir": "webapp"
      }
    ]
  }